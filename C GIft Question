#include<stdio.h>
void main() {
    int n,x,flag=0;
    printf("How many gifts you got: ");
    scanf("%d", &n);
    printf("Which one you want to find: ");
    scanf("%d", &x);
    int a[n];
    printf("Enter elements: ");
    for(int i=0;i<n;i++){
        scanf("%d", &a[i]);
        if(a[i]==x) flag = 1;
    }
    if(flag==1) printf("YES");
    else printf("NO");
}
