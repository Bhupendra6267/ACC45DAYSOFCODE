// Write a C program to find power of a number using for loop.

#include<stdio.h>
void main() {
    int n,p;
    printf("Enter a base: ");
    scanf("%d", &n);
    printf("Enter a power: ");
    scanf("%d", &p);
    int ans=n;
    for(int i=1;i<p;i++){
        ans *= n;
    }
    printf("%d to the power %d is %d", n,p,ans);
}   

