#include<stdio.h>
void PrintArray(int [],int);
void main(){
    int a[] = {5,7,2,1,9,7,4,2};
    int i,e,p,s;
    s = sizeof(a)/sizeof(a[0]);
printf("Enter the element you want to add: ");
    scanf("%d", &e);
printf("Enter the place you want to add: ");
scanf("%d", &p);
for (i = s; i >= p; i--)
		a[i] = a[i - 1];
	a[p - 1] = e;
    PrintArray(&a,s);
}
void PrintArray(int a[],int s){
    printf("Array : ");
    for(int i=0;i<=s;i++)
        printf("%d ", a[i]);
   Â printf("\n");
}
